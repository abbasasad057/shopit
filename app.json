{
  "expo": {
    "name": "ShopIt",
    "slug": "ShopIt",
    "privacy":"public",
    "description":"ShopIt is your smart shopping assistant",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.asad.shopit"
    },
    "web": {
      "favicon": "./assets/icon.png"
    }
  }
}
